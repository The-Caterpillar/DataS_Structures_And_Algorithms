// RECURSION
#include <bits/stdc++.h>
using namespace std;

void print(int n)
{
    if(n==0) return;
    
    cout<<n<<endl;
    print(n-1);
}

int main()
{
    int n; cin>>n;
    
    print(n);
    
    return 0;
}


// BACKTRACKING
#include <bits/stdc++.h>
using namespace std;

void print(int n, int x=1)
{
    if(x>n) return;
    
    print(n,x+1);
    cout<<x<<endl;
}

int main()
{
    int n; cin>>n;
    
    print(n);
    
    return 0;
}
